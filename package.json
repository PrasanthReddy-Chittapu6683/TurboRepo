{
  "private": true,
  "scripts": {
    "build": "turbo run build --no-daemon",
    "dev": "turbo run dev --no-cache --continue --no-daemon",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "name": "turborepo",
  "packageManager": "npm@8.10.0",
  "workspaces": ["apps/*", "packages/*"]
}
